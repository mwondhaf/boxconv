{
  "name": "boxconv",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "npx convex dev --once && concurrently -r npm:dev:web npm:dev:convex",
    "dev:web": "vite dev",
    "dev:convex": "npx convex dev",
    "build": "vite build && tsc --noEmit",
    "start": "node .output/server/index.mjs",
    "format": "prettier --write .",
    "lint": "tsc && eslint .  --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "check": "ultracite check",
    "fix": "ultracite fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@clerk/tanstack-react-start": "^0.28.1",
    "@convex-dev/react-query": "0.1.0",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-router": "^1.157.18",
    "@tanstack/react-router-devtools": "^1.157.18",
    "@tanstack/react-router-with-query": "^1.130.17",
    "@tanstack/react-start": "^1.157.18",
    "convex": "^1.31.7",
    "nitro": "npm:nitro-nightly@latest",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "redaxios": "^0.5.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@convex-dev/eslint-plugin": "^1.1.1",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/eslint-config": "^0.3.4",
    "@types/node": "^24.10.9",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "autoprefixer": "^10.4.24",
    "concurrently": "~9.2.1",
    "husky": "^9.1.7",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "ultracite": "^7.1.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.5"
  }
}
